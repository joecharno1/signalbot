# Signal Idle User Bot Environment Configuration
# Copy this file to .env and update with your values

# Bot's Signal phone number (must be registered)
BOT_PHONE_NUMBER=+1234567890

# Signal CLI REST API service address
SIGNAL_SERVICE=127.0.0.1:8080

# Admin phone numbers (comma-separated)
ADMIN_NUMBERS=+1234567890,+0987654321

# Bot configuration
IDLE_THRESHOLD_DAYS=30
DRY_RUN=true
LOG_LEVEL=INFO

